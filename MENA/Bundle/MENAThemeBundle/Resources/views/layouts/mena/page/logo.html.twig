{% block _logo_widget %}
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NNSQRZF"
                      height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->

    {% if app.request.attributes.get('_route') == 'oro_frontend_root' %}
        <img
            src="{% image '@MENAThemeBundle/Resources/public/frontendbundle/images/logo/logo.jpg' %}{{ asset_url }}{% endimage %}"
            class="logo"
            style = "width: 200px; height: 40px;"
            alt="logo"
        >
    {% else %}
        <a href="{{ path('oro_frontend_root') }}" class="logo" style="background: url('{% image '@MENAThemeBundle/Resources/public/frontendbundle/images/logo/logo.jpg' %}{{ asset_url }}{% endimage %}') no-repeat; background-position: center;">TruPar</a>
    {% endif %}
{% endblock %}